 <div class='nested-fields panel panel-default step_panel'>
  <div class="panel-heading">
    <h4 class="panel-title">Step <%= step.order %>
      <span class="pull-right "><i id="step<%= step.order %>" class="glyphicon glyphicon-chevron-up unique clickable"></i></span>
    </h4>
  </div>
  <div class="panel-body">
   <div class="row">
      <div id='field_definition'>
        <div class="col-sm-12">
          <%= render partial: 'task_field', collection: step.fields, as: :field%>
        </div>
      </div>
    </div>
  </div>
 </div>
